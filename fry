-- 功能按钮生成器（缩小尺寸）
local function createFeatureButton(text, yPos)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0.9, 0, 0, 32) -- 缩小高度
    btn.Position = UDim2.new(0.05, 0, 0, yPos)
    btn.BackgroundColor3 = Color3.fromRGB(70, 70, 100)
    btn.TextColor3 = Color3.fromRGB(240, 240, 255)
    btn.Font = Enum.Font.GothamMedium
    btn.Text = text
    btn.TextSize = 14 -- 缩小字体
    btn.AutoButtonColor = false
    btn.Parent = FeaturePage1
    
    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0, 6)
    btnCorner.Parent = btn
    
    btn.MouseEnter:Connect(function()
        btn.BackgroundColor3 = Color3.fromRGB(90, 90, 130)
    end)
    
    btn.MouseLeave:Connect(function()
        btn.BackgroundColor3 = Color3.fromRGB(70, 70, 100)
    end)
    
    return btn
end

-- 创建功能按钮（调整间距）
local speedBtn = createFeatureButton("人物速度加速", 10)
local infJumpBtn = createFeatureButton("无限跳跃", 90)
local noclipBtn = createFeatureButton("穿墙", 130)
local myScriptBtn = createFeatureButton("飞行", 210)

local function safeConnect(button, action)
    local debounce = false
    local function safeAction()
        if not debounce then
            debounce = true
            action()
            task.wait(0.1)
            debounce = false
        end
    end
    
    button.MouseButton1Click:Connect(safeAction)
    button.TouchTap:Connect(safeAction)
end

safeConnect(speedBtn, function()
    local char = LP.Character
    if char then
        local hum = char:FindFirstChildOfClass("Humanoid")
        if hum then
            hum.WalkSpeed = 50
            speedBtn.Text = "速度加速 (50)"
        end
    end
end)

safeConnect(infJumpBtn, function()
    _G.InfJumpEnabled = not _G.InfJumpEnabled
    infJumpBtn.Text = _G.InfJumpEnabled and "无限跳跃 (开启)" or "无限跳跃"
end)

UIS.JumpRequest:Connect(function()
    if _G.InfJumpEnabled then
        local char = LP.Character
        if char then
            local hum = char:FindFirstChildOfClass("Humanoid")
            if hum then
                hum:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end
    end
end)

safeConnect(noclipBtn, function()
    _G.NoClipEnabled = not _G.NoClipEnabled
    noclipBtn.Text = _G.NoClipEnabled and "穿墙模式 (开启)" or "穿墙模式"
    
    if _G.NoClipEnabled then
        coroutine.wrap(function()
            while _G.NoClipEnabled and task.wait(0.1) do
                local char = LP.Character
                if char then
                    for _, part in ipairs(char:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end
            end
        end)()
    end
end)

