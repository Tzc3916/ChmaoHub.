Button(Tab9, "神青高级自瞄", function() 
    shin_qine="作者神青" 
shin__qine="高级自瞄" 
loadstring(game:HttpGet("https://raw.githubusercontent.com/gycgchgyfytdttr/QQ-9-2-8-9-50173/refs/heads/main/cure.lua"))()
